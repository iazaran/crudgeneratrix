{
    "name": "iazaran/crudgeneratrix",
    "description": "PHP CRUD Generator",
    "keywords": ["PHP"],
    "homepage": "https://github.com/iazaran/crudgeneratrix",
    "license": "MIT",
    "authors": [
        {
            "name": "Ismael Azaran",
            "email": "eazaran@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/iazaran/crudgeneratrix/issues"
    },
    "autoload": {
        "psr-4": {
            "iazaran\\crudgeneratrix\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform": {
            "php": "8.1"
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-mysqli": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
